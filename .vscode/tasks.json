{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "maven build",
			"type": "shell",
			"command": "./mvnw",
			"args": [
				"compile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$javac"
			],
			"presentation": {
				"reveal": "silent"
			}
		}
	]
}